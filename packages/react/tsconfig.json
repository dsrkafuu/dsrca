{
  "extends": "@dsrca/config",
  "compilerOptions": {
    "noEmit": false,
    "emitDeclarationOnly": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@dsrca/react": ["src"],
      "@dsrca/react/*": ["src/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": [".dumi/**/*", ".dumirc.ts", "./src"]
}
